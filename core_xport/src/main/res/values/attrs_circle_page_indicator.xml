<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 圆形Page指示器 -->
    <declare-styleable name="CirclePageIndicator">
        <attr name="cpi_size" format="integer" />
        <attr name="cpi_position" format="integer"/>
        <attr name="cpi_radius" format="dimension|reference" />
        <attr name="cpi_unselect_color" format="color|reference" />
        <attr name="cpi_select_color" format="color|reference" />
        <attr name="cpi_ratio" format="float" />
    </declare-styleable>
</resources>